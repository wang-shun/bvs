<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.equinox.http.registry.resources">
      <resource
            alias="/resource"
            base-name="resource">
      </resource>
      <resource
            alias="/bruik.appcache"
            base-name="resource/bruik.appcache">
      </resource>
   </extension>
   <extension
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/bvs/fs"
            class="com.bizvisionsoft.bruiengine.servlet.Download">
      </servlet>
      <servlet
            alias="/bvs/svg"
            class="com.bizvisionsoft.bruiengine.servlet.SVGGenerator">
      </servlet>
   </extension>

</plugin>
